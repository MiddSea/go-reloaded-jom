  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },


    {
      "name": "Launch with sample.txt + result.txt Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "sample.txt",
        "result.txt"
        ]
    },
    {
      "name": "Launch with sampleTesting + resultsTesting.txt Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "sampleTesting.txt",
        "resultsTesting.txt"
        ]
    },
    {
      "name": "punchRegEx",
      "type": "go",
      "program": "${workspaceFolder}experiment/punchRegEx/punchRegEx.go",
      
      "request": "launch",
      "mode": "auto"
    }
  ]
}
